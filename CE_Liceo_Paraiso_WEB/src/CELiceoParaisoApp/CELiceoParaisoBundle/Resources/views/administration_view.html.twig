<!doctype html>
<html lang="en">
    <head>
        <meta charset="utf-8" />
        {#<link rel="apple-touch-icon" sizes="76x76" href="assets/img/apple-icon.png"> ICONO DE LA PÁGINA
        <link rel="icon" type="image/png" sizes="96x96" href="assets/img/favicon.png">#}
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
        <title>CELIPA Admin</title>
        <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' name='viewport' />
        <meta name="viewport" content="width=device-width" />
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
        <script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDjY6bb7os43WpETqE-dSKsGkLF1wTzqYk&callback"></script>
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
        <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
        <!-- Animation library for notifications   -->
        <link href="{{asset('bundles/ce_liceo_paraiso_app/admin/css/animate.min.css')}}" rel="stylesheet"/>
        <!--  Paper Dashboard core CSS    -->
        <link href="{{asset('bundles/ce_liceo_paraiso_app/admin/css/paper-dashboard.css')}}" rel="stylesheet"/>
        <!--  CSS for Demo Purpose, don't include it in your project     -->
        <link href="{{asset('bundles/ce_liceo_paraiso_app/admin/css/demo.css')}}" rel="stylesheet" />

        <!--  Fonts and icons-->
        <link href="http://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css" rel="stylesheet">
        <link href='https://fonts.googleapis.com/css?family=Muli:400,300' rel='stylesheet' type='text/css'>
        <link href="{{asset('bundles/ce_liceo_paraiso_app/admin/css/themify-icons.css')}}" rel="stylesheet">

        <!--   Core JS Files  -->
        <!--  Checkbox, Radio & Switch Plugins -->
        <script src="{{asset('bundles/ce_liceo_paraiso_app/admin/js/bootstrap-checkbox-radio.js')}}"></script>
        <!--  Charts Plugin -->
        <script src="{{asset('bundles/ce_liceo_paraiso_app/admin/js/chartist.min.js')}}"></script>
        <!--  Notifications Plugin    -->
        <script src="{{asset('bundles/ce_liceo_paraiso_app/admin/js/bootstrap-notify.js')}}"></script>
        <!-- Paper Dashboard Core javascript and methods for Demo purpose -->
        <script src="{{asset('bundles/ce_liceo_paraiso_app/admin/js/paper-dashboard.js')}}"></script>
        <!-- Paper Dashboard DEMO methods, don't include it in your project! -->
        <script src="{{asset('bundles/ce_liceo_paraiso_app/admin/js/demo.js')}}"></script>
        <script src="{{asset('bundles/ce_liceo_paraiso_app/admin/js/functions.js')}}"></script>

        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    </head>
    <body>
        <div class="wrapper">
            <div class="sidebar" data-background-color="black" data-active-color="danger">
                <div class="sidebar-wrapper" >
                    <div class="logo active">
                        <a href="{{ path('ce_liceo_paraiso_dashboard') }}" class="simple-text">
                            CELIPA Admin
                        </a>
                    </div>
                    <ul class="nav">
                        {% block menu %} {% endblock %}
                    </ul>
                </div>
            </div>

            <div class="main-panel">
                <nav class="navbar navbar-ct-danger">
                    <div class="container-fluid">
                        <div class="navbar-header">
                            <button type="button" class="navbar-toggle">
                                <span class="sr-only">Toggle navigation</span>
                                <span class="icon-bar bar1"></span>
                                <span class="icon-bar bar2"></span>
                                <span class="icon-bar bar3"></span>
                            </button>
                            <a class="navbar-brand" href="#">{% block title %}{% endblock %}</a>
                        </div>
                        <div class="collapse navbar-collapse">
                            <ul class="nav navbar-nav navbar-right">
                                <li>
                                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                        <i class="ti-user"></i>
                                        <p>Perfil</p>
                                    </a>
                                </li>
                                <li class="dropdown">
                                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                        <i class="ti-settings"></i>
                                        <p>Configuración</p>
                                        <b class="caret"></b>
                                    </a>
                                    <ul class="dropdown-menu">
                                        <li><a href="{{ path('ce_liceo_paraiso_change_pass_form') }}">Cambiar clave</a></li>
                                        <li><a href="{{ path('ce_liceo_paraiso_logout') }}">Cerrar Sesión</a></li>
                                    </ul>
                                </li>
                            </ul>
                        </div>
                    </div>
                </nav>
                <div class="content">
                    <div class="container-fluid">
                        {% block content %} {% endblock %}
                    </div>
                </div>
                <footer class="footer">
                    <div class="container-fluid">
                        <nav class="pull-left">
                            <ul>
                                <li>
                                    <a href="#">
                                        <b>Trabajo Comunal Universitario 563 - UCR, Sede del Atlántico, Recinto Paraíso</b>
                                    </a>
                                </li>
                            </ul>
                        </nav>
                        <div class="copyright pull-right">
                            &copy; <script>document.write(new Date().getFullYear());</script>, Proyecto TCU - <a href="https://www.ucr.ac.cr/">UCR</a>
                        </div>
                    </div>
                </footer>
            </div>
        </div>
        {% block modals %} {% endblock %}
    </body>
</html>
{% block scripts %} {% endblock %}